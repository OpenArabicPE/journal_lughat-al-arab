   <bib:Article rdf:about="#oclc_472450345-i_85-div_15.d2e3535">
      <z:itemType>magazineArticle</z:itemType>
      <dcterms:isPartOf>
         <bib:Periodical>
            <prism:volume>8</prism:volume>
